{
	"$schema": "https://raw.githubusercontent.com/Bedrock-OSS/regolith-schemas/main/config/v1.4.json",
	"author": "Shapescape",
	"name": "Educator-Tools",
	"packs": {
		"behaviorPack": "./packs/BP",
		"resourcePack": "./packs/RP"
	},
	"regolith": {
		"dataPath": "filters_data",
		"filterDefinitions": {
			"debug_say_function_name": {
				"url": "github.com/Nusiq/regolith-filters",
				"version": "2.0.0"
			},
			"modular_mc": {
				"url": "github.com/Nusiq/regolith-filters",
				"version": "1.1.0"
			},
			"name_ninja": {
				"url": "github.com/Bedrock-OSS/regolith-filters",
				"version": "1.3.1"
			},
			"shapescape_copy_files": {
				"url": "github.com/ShapescapeMC/Shapescape-Copy-Files",
				"version": "1.0.0"
			},
			"subfunctions": {
				"url": "github.com/Nusiq/regolith-filters",
				"version": "2.1.2"
			},
			"texture_list": {
				"url": "github.com/Bedrock-OSS/regolith-filters",
				"version": "1.1.3"
			}
		},
		"formatVersion": "1.4.0",
		"profiles": {
			"bedrock": {
				"export": {
					"build": "standard",
					"readOnly": false,
					"target": "development"
				},
				"filters": [
					{
						"profile": "default"
					}
				]
			},
			"bedrock_more_letter_blocks": {
				"export": {
					"build": "standard",
					"readOnly": false,
					"target": "development"
				},
				"filters": [
					{
						"profile": "more_letter_blocks"
					}
				]
			},
			"debug": {
				"export": {
					"build": "standard",
					"readOnly": false,
					"target": "development"
				},
				"filters": [
					{
						"profile": "default"
					},
					{
						"filter": "debug_say_function_name",
						"settings": {
							"exclude": [],
							"include": [
								"**/*.mcfunction"
							],
							"prefix": "",
							"random_colors": true
						}
					}
				]
			},
			"default": {
				"export": {
					"build": "education",
					"readOnly": false,
					"target": "development"
				},
				"filters": [
					{
						"profile": "educator_tools"
					}
				]
			},
			"educator_tools": {
				"export": {
					"build": "education",
					"readOnly": false,
					"target": "development"
				},
				"filters": [
					{
						"filter": "modular_mc",
						"settings": {
							"whitelist": [
								"educator_tools",
								"letter_blocks",
								"scripting_setup",
								"addon_setup/educator_tools"
							],
							"esbuild": {
								"buildPath": ".modular_mc/main.js",
								"settings": {
									"outfile": "BP/scripts/edu_tools/main.js",
									"sourcemap": false,
									"minify": false,
									"external": ["@minecraft/server", "@minecraft/server-ui"]
								}
							}
						}
					},
					{
						"filter": "subfunctions"
					},
					{
						"filter": "name_ninja",
						"settings": {
							"blocks": {
								"auto_name": true
							},
							"entities": {
								"auto_name": true
							},
							"items": {
								"auto_name": true
							},
							"spawn_eggs": {
								"auto_name": true,
								"postfix": "",
								"prefix": "§fSpawn §f"
							}
						}
					},
					{
						"filter": "texture_list"
					}
				]
			},
			"more_letter_blocks": {
				"export": {
					"build": "education",
					"readOnly": false,
					"target": "development"
				},
				"filters": [
					{
						"filter": "modular_mc",
						"settings": {
							"whitelist": [
								"addon_setup/more_letter_blocks",
								"more_letter_blocks"
							],
							"esbuild": {
								"buildPath": ".modular_mc/main.js",
								"settings": {
									"outfile": "BP/scripts/more_letter_blocks/main.js",
									"sourcemap": false,
									"minify": false,
									"external": ["@minecraft/server", "@minecraft/server-ui"]
								}
							}
						}
					},
					{
						"filter": "subfunctions"
					},
					{
						"filter": "name_ninja",
						"settings": {
							"blocks": {
								"auto_name": true
							},
							"entities": {
								"auto_name": true
							},
							"items": {
								"auto_name": true
							},
							"spawn_eggs": {
								"auto_name": true,
								"postfix": "",
								"prefix": "§fSpawn §f"
							}
						}
					},
					{
						"filter": "texture_list"
					}
				]
			},
			"packaging_educator_tools": {
				"export": {
					"bpPath": "../behavior_packs/0",
					"readOnly": false,
					"rpPath": "../resource_packs/0",
					"target": "exact"
				},
				"filters": [
					{
						"profile": "default"
					}
				]
			},
			"packaging_more_letter_blocks": {
				"export": {
					"bpPath": "../behavior_packs/1",
					"readOnly": false,
					"rpPath": "../resource_packs/1",
					"target": "exact"
				},
				"filters": [
					{
						"profile": "more_letter_blocks"
					}
				]
			}
		}
	}
}
